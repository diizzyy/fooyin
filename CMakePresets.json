{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 18,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "base",
      "displayName": "base preset",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build-${presetName}",
      "hidden": true
    },
    {
      "name": "vcpkg",
      "displayName": "vcpkg preset",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": {
          "type": "FILEPATH",
          "value": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake"
        },
        "INSTALL_FHS": "OFF"
      },
      "inherits": [
        "base"
      ]
    },
    {
      "name": "dev",
      "displayName": "Build as debug",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "BUILD_TESTING": "ON"
      },
      "inherits": [
        "base"
      ]
    },
    {
      "name": "dev-clang",
      "displayName": "dev-clang",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "BUILD_TESTING": "ON"
      },
      "environment": {
        "CXX": "clang++",
        "CCACHE_DISABLE": "ON"
      },
      "inherits": [
        "base"
      ]
    },
    {
      "name": "dev-vcpkg",
      "displayName": "dev-vcpkg",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "BUILD_TESTING": false
      },
      "inherits": [
        "vcpkg"
      ]
    },
    {
      "name": "release",
      "displayName": "Build as release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      },
      "inherits": [
        "base"
      ]
    },
    {
      "name": "release-vcpkg",
      "displayName": "release-vcpkg",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      },
      "inherits": [
        "vcpkg"
      ]
    },
    {
      "name": "profile",
      "displayName": "profile",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "RelWithDebInfo"
      },
      "inherits": [
        "base"
      ]
    },
    {
      "hidden": true,
      "name": "Qt",
      "cacheVariables": {
        "CMAKE_PREFIX_PATH": "$env{QTDIR}"
      },
      "environment": {
        "PATH": "$penv{PATH};$env{QTDIR}/bin"
      },
      "vendor": {
        "qt-project.org/Qt": {
          "checksum": "Sp19WUpde73JS7nK33HZMqVZTyI="
        }
      }
    }
  ],
  "buildPresets": [
    {
      "name": "dev",
      "configurePreset": "dev",
      "targets": [
        "all"
      ]
    },
    {
      "name": "dev-clang",
      "configurePreset": "dev-clang"
    },
    {
      "name": "release",
      "configurePreset": "release"
    }
  ],
  "vendor": {
    "qt-project.org/Presets": {
      "checksum": "GjVwuvU0ZMQ4C+wqeGdgJtvPS4U="
    }
  }
}